<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vincent.template.mapper.FieldDataMapper">

        <!-- 通用查询映射结果 -->
        <resultMap id="BaseResultMap" type="com.vincent.template.entity.FieldData">
                <result column="id" property="id" />
                    <result column="template_id" property="templateId" />
                    <result column="field_data" property="fieldData" />
                    <result column="remark" property="remark" />
                    <result column="obj_id" property="objId" />
        </resultMap>

        <!-- 通用查询结果列 -->
        <sql id="Base_Column_List">
                id,
            template_id, field_data, remark, obj_id
        </sql>

</mapper>
